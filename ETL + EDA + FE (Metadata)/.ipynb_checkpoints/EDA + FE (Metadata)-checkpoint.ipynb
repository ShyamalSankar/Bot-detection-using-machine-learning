    "## Feature Engineering\n",
    "* Train - Test Split\n",
    "* Standard Scaler"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "771b2e58",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Getting X and y\n",
    "x = df_metadata.drop(columns = ['isBot', 'tweet_count_log', 'followers_count_log', 'following_count_log', 'listed_count_log', \n",
    "                                'des_no_of_usertags_log', 'des_no_of_hashtags_log'])\n",
    "y = df_metadata['isBot']\n",
    "\n",
    "# Split into training and testing sets before scaling the variables and performing one hot encoding to avoid data leakage\n",
    "x_train, x_test, y_train, y_test = train_test_split(x, y, test_size = 0.2, random_state = 101)\n",
    "print(\"Train set:\", x_train.shape, y_train.shape)\n",
    "print(\"Test set:\", x_test.shape, y_test.shape)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2c89f3f8",
   "metadata": {},
   "outputs": [],
   "source": [
    "scaler = StandardScaler()\n",
    "numeric_variables = ['followers_count', 'following_count', 'listed_count', 'tweet_count', 'un_no_of_char','name_no_of_char', 'des_no_of_usertags', 'des_no_of_hashtags']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a3793d96",
   "metadata": {},
   "outputs": [],
   "source": [
    "x_train[numeric_variables] = scaler.fit_transform(x_train[numeric_variables])\n",
    "x_test[numeric_variables] = scaler.transform(x_test[numeric_variables])\n",
    "\n",
    "train_dataset = x_train.copy()\n",
    "test_dataset = x_test.copy()\n",
    "\n",
    "y_train = pd.DataFrame(y_train)\n",
    "y_test = pd.DataFrame(y_test)\n",
    "\n",
    "train_dataset[\"isBot\"] = y_train[\"isBot\"]\n",
    "test_dataset[\"isBot\"] = y_test[\"isBot\"]"
   ]
